            <ColumnDefinition Width="93*"/>
            <ColumnDefinition Width="50*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="labelTitle" Content="Gestión de avituallamientos" HorizontalAlignment="Center" HorizontalContentAlignment ="Center"
               Margin="10,10,10,0" VerticalAlignment="Top" Width="266" FontWeight="Bold" Grid.ColumnSpan="2"/>
        <Button x:Name="buttonCrear" Content="Crear avituallamientos" HorizontalAlignment="Center
                " Margin="74,36,74,0" VerticalAlignment="Top" Width="138" Grid.ColumnSpan="2" Click="buttonCrear_Click"/>
        <Button x:Name="ButtonPrueba" Content="Prueba" HorizontalAlignment="Center" Margin="74,61,74,0" VerticalAlignment="Top" Width="138" Click="ButtonPrueba_Click" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
